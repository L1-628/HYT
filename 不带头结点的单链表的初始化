typedef struct LNode{//定义单链表的结点类型 
	ElemTpe data;//每个结点存放一个数据元素 
	struct LNode *next;//指针指向下一结点 
}LNode,*LinkList; 
//初始化一个空的链表 
bool InitList(LinkList L,&L){
	L=NULL;//空表，暂时还没有任何结点 ，防止脏数据
	return true;
}
bool Empty(LinkList L){
	if(L==NULL){
		return true;
	}
	else{
		return false;
	}
}
void test(){
	LinkList L;//声明一个指向单链表的指针，此处并没有创建一个结点
	//初始化一个空表 
	InitList();
}
