#include<stdio.h>
//assert的头文件
#include<assert.h>
void my_strcopy(char* dest, char* scr) {
	//断言scr不等于NULL
	assert(scr != NULL);
	while (*dest++ = *scr++) {
		;
	}
}
int main() {
	char arr1[10] = "XXXXXXXXX";
	char arr2[] = "hello";
	/*char* arr3 = NULL;*/
	my_strcopy(arr1, arr2);
	printf("%s\n", arr1);
	return 0;
}
