#define MaxSize 10//定义栈中元素的最大个数
typedef struct {
	int data[MaxSize];//静态数组存放栈中元素
	int top;//栈顶指针
}SqStack;
//初始化栈
void InitStack(SqStack& S) {
	S.top = -1;//初始化栈顶指针
}
//判断栈空
bool StackEmpty(SqStack S) {
	if (S.top == -1) {//栈空
		return true;
	}
	else {//不空
		return false;
	}
}
void testStack() {
	SqStack S;//声明一个顺序栈（分配空间）
	InitStack(S);
	//。。。后续操作。。。
}
